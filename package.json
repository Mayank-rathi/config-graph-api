{
  "name": "config-graph-api",
  "version": "1.0.0",
  "description": "config-graph-api",
  "scripts": {
    "copy-schemas": "cd infrastructure/functions && copyfiles ./*.graphql ./dist/CONFIG_GRAPH_API/schemas",
    "test": "jest",
    "start": "concurrently --kill-others \"npm run fnStart\" \"npm run watch\"",
    "fnStart": "cd infrastructure/functions/ && func start",
    "build:production": "npm run prestart && npm prune --production",
    "prestart": "npm run build && func extensions install",
    "build:prod": "webpack --mode=development  && npm run copy-schemas",
    "build": "tsc && yarn run copy-schemas",
    "publish:fn": "cd infrastructure/functions/ && func azure functionapp publish config-graph-dev-us-c-fn",
    "watch": "webpack --mode=development --watch",
    "serve": "nodemon --watch '**/*.ts' --exec \"ts-node\" infrastructure/api/app.ts",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@azure/functions": "^3.0.0",
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.1",
    "apollo-server-azure-functions": "^3.6.7",
    "copyfiles": "^2.4.1",
    "graphql": "^16.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^8.2.4",
    "graphql-type-json": "^0.3.2",
    "gremlin": "^3.3.4",
    "http": "^0.0.1-security",
    "path": "^0.12.7",
    "typescript": "^4.0.0",
    "webpack": "^5.53.0"
  },
  "devDependencies": {
    "native-ext-loader": "^2.3.0",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack-cli": "^4.8.0"
  },
  "main": "index.js"
}
